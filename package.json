{
  "name": "fmentor-nft-react-vite",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "sass-dev": "sass --watch src/assets/scss/main.scss src/assets/css/main.css",
    "sass-prod": "sass src/assets/scss/main.scss src/assets/css/main.css --style=compressed --no-source-map",
    "postcss": "postcss src/assets/css/main.css -o src/assets/css/main.css --use autoprefixer --no-map",
    "prettier": "prettier --write .",
    "dev": "run-s sass-dev",
    "lint": "run-s prettier",
    "build-css": "run-s lint sass-prod postcss",
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "postcss-cli": "^10.1.0",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "vite": "^5.0.8"
  }
}
